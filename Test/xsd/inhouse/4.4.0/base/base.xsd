        <xs:annotation>
            <xs:documentation xml:lang="en">
       
      		</xs:documentation>
        </xs:annotation>
        <xs:choice>
			<xs:element name="messageESEP" type="messageESEP:MessageESEPType"/>
		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="FtboxResponseSPRIType">
