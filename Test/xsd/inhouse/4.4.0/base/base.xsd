	<xs:complexType name="CustomerRequestMessageType">
		<xs:sequence>
			<xs:element name="baseMessage" type="base:BaseMessageCustomerRequestType" />
			<xs:choice>
  				<xs:element name="WITA" type="base:CustomerRequestWITAType" />
 				<xs:element name="ESEP" type="base:CustomerRequestESEPType" />
				<xs:element name="SPRI" type="base:CustomerRequestSPRIType" />
				<xs:element name="WBCI" type="base:CustomerRequestWBCIType" />
 				<xs:element name="ESS" type="base:CustomerRequestESSType"/>
				<xs:element name="VINE" type="base:CustomerRequestVineType"/>
 				<xs:element name="WAR" type="base:CustomerRequestWARType"/>
				<xs:element name="ACS" type="base:CustomerRequestACSType"/>
				<xs:element name="TIM" type="base:CustomerRequestTIMType"/>
 			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="FtboxResponseMessageType">
		<xs:sequence>
			<xs:element name="baseMessage" type="base:BaseMessageFtboxResponseType" />                        
            <xs:choice>
             	<xs:element name="WITA" type="base:FtboxResponseWITAType" />
				<xs:element name="ESEP" type="base:FtboxResponseESEPType" />
				<xs:element name="SPRI" type="base:FtboxResponseSPRIType" />
				<xs:element name="WBCI" type="base:FtboxResponseWBCIType" />
				<xs:element name="ESS" type="base:FtboxResponseESSType"/>
				<xs:element name="VINE" type="base:FtboxResponseVineType"/>
 				<xs:element name="WAR" type="base:FtboxResponseWARType"/>
				<xs:element name="ACS" type="base:FtboxResponseACSType" />
				<xs:element name="TIM" type="base:FtboxResponseTIMType" />
            </xs:choice>
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="FtboxRequestMessageType">
        <xs:sequence>
            <xs:element name="baseMessage" type="base:BaseMessageFtboxRequestType" /> 
            <xs:choice>
            	<xs:element name="WITA" type="base:FtboxRequestWITAType" />
				<xs:element name="ESEP" type="base:FtboxRequestESEPType" />
				<xs:element name="SPRI" type="base:FtboxRequestSPRIType" />
				<xs:element name="WBCI" type="base:FtboxRequestWBCIType" />
 				<xs:element name="VINE" type="base:FtboxRequestVineType"/>
 				<xs:element name="ACS" type="base:FtboxRequestACSType"/>
 				<xs:element name="TIM" type="base:FtboxRequestTIMType"/>
           </xs:choice> 
         </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CustomerResponseMessageType">
        <xs:sequence>
            <xs:element name="baseMessage" type="base:BaseMessageCustomerResponseType" /> 
            <xs:choice>
            	<xs:element name="WITA" type="base:CustomerResponseWITAType" />
				<xs:element name="ESEP" type="base:CustomerResponseESEPType" />
				<xs:element name="SPRI" type="base:CustomerResponseSPRIType" />
				<xs:element name="WBCI" type="base:CustomerResponseWBCIType" />
            </xs:choice> 
        </xs:sequence>
    </xs:complexType>
		
	<xs:complexType name="ErrorResponseMessageType">
		<xs:sequence>
			<xs:element name="baseMessage" type="base:BaseMessageErrorType" />
			<xs:element name="errorMessage" maxOccurs="unbounded" type="base:ErrorMessageType" />
			<xs:element name="payload" minOccurs="0" type="base:PayloadType" />
		</xs:sequence>
	</xs:complexType>
		
	<xs:complexType name="ErrorMessageType">
		<xs:sequence>
			<xs:element name="code" type="base:AnswerCodeType"/>
			<xs:element name="message" type="base:AnswerMessageType" minOccurs="0"/>
		</xs:sequence>		
	</xs:complexType>
	
	<xs:complexType name="TechnicalReceiptType">
        <xs:sequence>
            <xs:element name="message" type="base:TEQMessageType"/>
            <xs:element name="code" type="base:TEQCodeType" minOccurs="0"/>
            <xs:element name="text" type="base:TEQTextType" minOccurs="0"/>
        </xs:sequence>      
    </xs:complexType>
	
	<xs:complexType name="OrderCancellationType">          
        <xs:sequence>
            <xs:element name="orderToCancel" type="base:OrderToCancelType"/>
        </xs:sequence>
    </xs:complexType>
	
	<xs:complexType name="OrderToCancelType">          
        <xs:sequence>
            <xs:element name="customerOrderId" type="base:CustomerOrderIdType"/>
            <xs:element name="ftboxOrderId" minOccurs="0" type="base:OrderIdType"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="OrderPostponementType">
        <xs:sequence>
            <xs:element name="orderToPostpone" type="base:OrderToPosteponeType"/>
        </xs:sequence>        
    </xs:complexType>
    
    <xs:complexType name="OrderToPosteponeType">
        <xs:sequence>
            <xs:element name="customerOrderId" type="base:CustomerOrderIdType"/>
            <xs:element name="ftboxOrderId" minOccurs="0" type="base:OrderIdType"/>
            <xs:element name="requestDeliveryDate" type="base:I015-TargetDateType"/>
            <xs:element name="installationServices" minOccurs="0" type="base:I014-InstallationServicesType"/>
        </xs:sequence>        
    </xs:complexType>       
    
	<xs:complexType name="BaseMessageCustomerRequestType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
        This is the standard element that each request communication act from inhouse-system towards the FTBOX shall have.
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="customerOrderId"
				type="base:CustomerOrderIdType" />
			<xs:element name="ftboxOrderId" minOccurs="0"
				type="base:OrderIdType" />
			<xs:element name="messageCorrelationId" type="base:MessageCorrelationIdType" maxOccurs="1" minOccurs="0"></xs:element>
			<xs:element name="carrier" type="base:CarrierType" />
			<xs:element name="provider" type="base:ProviderType" />
			<xs:element name="communicationDate"
				type="base:CommunicationDateType" />
			<xs:element name="transactionType"
				type="base:I001-TransactionCustomerRequestType" />
			<!--
				xs:documentation>date the message was
				generated/send</xs:documentation
			-->
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="BaseMessageFtboxResponseType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
        This is the standard element that each response message communication act from the FTBOX system to inhouse-system shall have.
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ftboxOrderId" minOccurs="0"
				type="base:OrderIdType" />
			<xs:element name="customerOrderId" minOccurs="0"
				type="base:CustomerOrderIdType" />
			<xs:element name="messageCorrelationId" type="base:MessageCorrelationIdType" maxOccurs="1" minOccurs="0"></xs:element>
			<xs:element name="carrier" type="base:CarrierType" />
			<xs:element name="provider" type="base:ProviderType" />
			<xs:element name="communicationDate"
				type="base:CommunicationDateType" />
			<xs:element name="transactionType"
				type="base:I001-TransactionFtboxResponseType" />
			<xs:element name="ftboxPosition" minOccurs="0"
				type="base:PositionType" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="BaseMessageErrorType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
        This is the standard element for each error message communication act from FTBOX to the in-house system and vise versa shall have.
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ftboxOrderId" minOccurs="0"
				type="base:OrderIdType" />
			<xs:element name="customerOrderId"
				type="base:CustomerOrderIdType" />
			<xs:element name="messageCorrelationId" type="base:MessageCorrelationIdType" maxOccurs="1" minOccurs="0"></xs:element>
			<xs:element name="communicationDate"
				type="base:CommunicationDateType" />
			<xs:element name="transactionType" minOccurs="0"
				type="base:I001-TransactionErrorResponseType" />
			<xs:element name="ftboxPosition" minOccurs="0"
				type="base:PositionType" />
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="BaseMessageFtboxRequestType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
        This is the standard element for each request from FTBOX to the in-house system shall have in case the FTBOX system initializes the workflow.
      </xs:documentation>
        </xs:annotation>
        <xs:sequence>
        	<xs:element name="ftboxOrderId" minOccurs="0"
        		type="base:OrderIdType" />
        	<xs:element name="customerOrderId" minOccurs="0"
        		type="base:CustomerOrderIdType" />
        	<xs:element name="messageCorrelationId" type="base:MessageCorrelationIdType" maxOccurs="1" minOccurs="0"></xs:element>
        	<xs:element name="contractId" minOccurs="0"
        		type="base:ContractIdType" />
        	<xs:element name="carrier" type="base:CarrierType" />
        	<xs:element name="provider" type="base:ProviderType" />
        	<xs:element name="communicationDate"
        		type="base:CommunicationDateType" />
        	<xs:element name="transactionType"
        		type="base:I001-TransactionFtboxRequestType" />
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="BaseMessageCustomerResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
        This is the standard element for each response from Customer to FTBOX shall have in case FTBOX initialized the workflow.
      </xs:documentation>
        </xs:annotation>
        <xs:sequence>
        	<xs:element name="customerOrderId" type="base:CustomerOrderIdType" />
        	<xs:element name="ftboxOrderId" minOccurs="0" type="base:OrderIdType" />
        	<xs:element name="messageCorrelationId" type="base:MessageCorrelationIdType" maxOccurs="1" minOccurs="0"></xs:element>
        	<xs:element name="contractId" minOccurs="0" type="base:ContractIdType" />
        	<xs:element name="carrier" type="base:CarrierType" />
        	<xs:element name="provider" type="base:ProviderType" />
        	<xs:element name="communicationDate" type="base:CommunicationDateType" />
        	<xs:element name="transactionType" type="base:I001-TransactionCustomerResponseType" />
        </xs:sequence>
    </xs:complexType>

	<xs:complexType name="CustomerRequestWITAType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
       
      		</xs:documentation>
        </xs:annotation>
        <xs:choice>
 			<xs:element name="orderTAL" type="orderTAL:OrderTALType" />
			<xs:element name="orderFTTH" type="orderFTTH:OrderFTTHType" />
			<xs:element name="orderXDSL" type="orderXDSL:OrderXDSLType"/>
            <xs:element name="orderXDSL-B" type="orderXDSL-B:OrderXDSL-BType"/>
		    <xs:element name="orderADSL" type="orderADSL:OrderADSLType"/>
		    <xs:element name="orderCLS" type="orderCLS:OrderCLSType"/>
		    <xs:element name="orderPRES" type="orderPRES:OrderPRESType"/>
 			<xs:element name="orderCancellation" type="base:OrderCancellationType"/>
			<xs:element name="orderPostponement" type="base:OrderPostponementType"/>
		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="CustomerRequestESEPType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
       
      		</xs:documentation>
        </xs:annotation>
        <xs:choice>
 			<xs:element name="orderESEP" type="orderESEP:OrderESEPType"/>
 			<xs:element name="orderCancellation" type="base:OrderCancellationType"/>
		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="CustomerRequestSPRIType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
       			
      		</xs:documentation>
        </xs:annotation>
        <xs:choice>
 			<xs:element name="orderFTTX" type="orderFTTX:OrderFTTX2Type"/>
        		<xs:element name="orderCancellation" type="base:OrderCancellationType"/>
			<xs:element name="orderPostponement" type="base:OrderPostponementType"/>
		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="CustomerRequestWBCIType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
       			
      		</xs:documentation>
        </xs:annotation>
        <xs:choice>
 			<xs:element name="orderWBCI-Auf" type="orderWBCI:OrderWBCIaufType"/>
       		<xs:element name="orderWBCI-Abg" type="orderWBCI:OrderWBCIabgType"/>
       		<xs:element name="messageWBCI" type="messageWBCI:MessageWBCIRequestType"/>
 		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="CustomerRequestESSType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
      		</xs:documentation>
      	</xs:annotation>
      	<xs:choice>
      		<xs:element name="orderTAL" type="orderESS:OrderTALType"/>
      		<xs:element name="orderXDSL" type="orderESS:OrderXDSLType"/>
      		<xs:element name="orderCancellation" type="base:OrderToCancelESSType"/>
      		<xs:element name="orderAnswerTAM" type="base:AnswerTAM_ESSType"/>
      	</xs:choice>
    </xs:complexType>
    
    <xs:complexType name="CustomerRequestVineType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
      		</xs:documentation>
      	</xs:annotation>
      	<xs:sequence>
      		<xs:element name="vineTTKey" minOccurs="0" type="orderVine:OrderVineKeyType"/>
      		<xs:element name="orderVine" type="orderVine:OrderVineType"/>
      	</xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CustomerRequestWARType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
      		</xs:documentation>
      	</xs:annotation>
      	<xs:choice>
       		<xs:element name="orderWAR" type="orderWAR:OrderWARType"/>
       	</xs:choice>
    </xs:complexType>
    
    <xs:complexType name="FtboxRequestACSType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
      		</xs:documentation>
      	</xs:annotation>
      	<xs:choice>
      		<xs:element name="messageACS" type="messageACS:requestACSType"/>
       	</xs:choice>
    </xs:complexType>
    
    <xs:complexType name="OrderToCancelESSType">
      	<xs:annotation>
      		<xs:documentation xml:lang="en">
      		</xs:documentation>
      	</xs:annotation>
      	<xs:complexContent>
      		<xs:extension base="base:OrderToCancelType">
      			<xs:sequence>
            		<xs:element name="customerContractData" type="segmentsESS:D002-CustomerContractData"/>
        			<xs:element name="resellerContractData" minOccurs="0" type="segmentsESS:D003-ResellerContractData"/> 			
      				<xs:element name="reference" type="segmentsESS:D025-FaultClearanceReferenceType"/>
      			</xs:sequence>
      		</xs:extension>
      	</xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="AnswerTAM_ESSType">
      	<xs:annotation>
      		<xs:documentation xml:lang="en">
      		</xs:documentation>
      	</xs:annotation>
			<xs:sequence>
			    <xs:element name="customerOrderId" type="base:CustomerOrderIdType"/>
	            <xs:element name="ftboxOrderId" minOccurs="0" type="base:OrderIdType"/>
	            <xs:element name="customerContractData" type="segmentsESS:D002-CustomerContractData"/>
        		<xs:element name="resellerContractData" minOccurs="0" type="segmentsESS:D003-ResellerContractData"/> 		
				<xs:element name="reference" type="segmentsESS:D025-FaultClearanceReferenceType"/>
				<xs:element name="requestDeliveryDate" type="segmentsESS:D015-TargetDateType"/>
			</xs:sequence>
    </xs:complexType> 
    
	
	<xs:complexType name="FtboxResponseWITAType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
       
      		</xs:documentation>
        </xs:annotation>
        <xs:choice>
			<xs:element name="messageTAL" type="messageTAL:MessageTALType"/>
			<xs:element name="messageFTTH" type="messageFTTH:MessageFTTHType"/>
            <xs:element name="messageXDSL" type="messageXDSL:MessageXDSLType"/>
            <xs:element name="messageXDSL-B" type="messageXDSL-B:MessageXDSL-BType"/>
            <xs:element name="messageADSL" type="messageADSL:MessageADSLType"/>
            <xs:element name="messageCLS" type="messageCLS:MessageCLSType"/>
            <xs:element name="messagePRES" type="messagePRES:MessagePRESType"/>
		</xs:choice>
	</xs:complexType>
	
