<?xml version="1.0" encoding="UTF-8"?>
<!-- $Revision: 47001 $ $LastChangedDate: 2014-03-13 13:04:16 +0100 (Do, 13 Mrz 2014) $ -->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:fields="http://www.flexagon.de/inhouse/fields/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://www.flexagon.de/inhouse/fields/"
    elementFormDefault="unqualified">
	
     <!-- modelling the phone number information -->
    <xs:complexType name="IptnType">
            <xs:sequence>
            <!-- cc = country code = Landesvorwahl -->
            <xs:element name="cc" minOccurs="0" type="fields:CcType" fixed="49"/>
            <!-- ndc = national destination code = Vorwahl -->
            <xs:element name="ndc" type="fields:NdcType"/>
            <!-- sn = subscriber number = Rufnummer -->
            <xs:element name="sn" type="fields:SnType"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="PersonType">
            <xs:sequence>
            	<xs:element name="givenName" minOccurs="0" type="fields:GivenNameType"/>
            	<xs:element name="surname" type="fields:SurNameType"/> 
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CompanyType">
            <xs:sequence>
            	<xs:element name="companyName" type="fields:GivenNameType"/>
            	<xs:element name="companyNamePart2"  minOccurs="0" type="fields:SurNameType"/>           	
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="PersonWithNumberType">
            <xs:sequence>
            	<xs:element name="givenName" minOccurs="0" type="fields:GivenNameType"/>
            	<xs:element name="surname" type="fields:SurNameType"/> 
            	<xs:element name="PhoneNumber" minOccurs="0" type="fields:PhoneNumberType"/> 
            	<xs:element name="mobileNumber" minOccurs="0" type="fields:PhoneNumberType"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CompanyWithNumberType">
            <xs:sequence>
            	<xs:element name="companyName" type="fields:GivenNameType"/>
            	<xs:element name="companyNamePart2"  minOccurs="0" type="fields:SurNameType"/>
            	<xs:element name="PhoneNumber" minOccurs="0" type="fields:PhoneNumberType"/> 
            	<xs:element name="mobileNumber" minOccurs="0" type="fields:PhoneNumberType"/>           	
        </xs:sequence>
    </xs:complexType>
    
    
    <!-- Geschaeftsfallart (W001.2) -->
    <xs:simpleType name="BusinessTransactionTypeType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the key for the choise of business case type  
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>            
        </xs:restriction>
    </xs:simpleType>

    <!-- Geschaeftsfall (W001.3)-->
    <xs:simpleType name="BusinessTransactionType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the key for the choise of business case
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Aenderungskennzeichen (W001.4) -->
    <xs:simpleType name="ChangeIndicatorType">
        <xs:annotation>
            <xs:documentation xml:lang="en">specifies an order as  Standard, Storno or Terminverschiebung</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>

     <!-- Art des Requests -->
    <xs:simpleType name="MessageCustomerRequestType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the type of the request
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
	
	<xs:simpleType name="MessageFtboxRequestType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the type of the request
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>

    <!-- Art der Response -->
    <xs:simpleType name="MessageFtboxResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the type of the response
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
    
	<xs:simpleType name="MessageCustomerResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the type of the response
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="MessageErrorResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the type of the response
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
 
    <!-- Art der Response -->
    <xs:simpleType name="MessageResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                the type of the response
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Kundennummer (W002.2/W003.2) -->
    <xs:simpleType name="CustomerIdType">
	    <xs:annotation>
	        <xs:documentation xml:lang="en">
	           the reseller/orderer identifier, in WITA it has to be 10 characters exactly, however, so pad with 0
	        </xs:documentation>
	    </xs:annotation>
	    <xs:restriction base="xs:string">
	        <xs:minLength value="10"/>
	        <xs:maxLength value="10"/>
	    </xs:restriction>
    </xs:simpleType>
    
    <!-- Rahmenvertragsnummer (W002.3/W003.3) -->
    <xs:simpleType name="CustomerServiceIdType">
    	<xs:annotation>
            <xs:documentation xml:lang="en">
               the master agreement identifier
            </xs:documentation>
        </xs:annotation>
    	<xs:restriction base="xs:string">
            <xs:minLength value="10"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
            
    <!-- Faktura  (W004.2) -->
    <xs:simpleType name="BookKeepingType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Nummer, die zur Fakturierung heranzuziehen ist
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Bestand  (W004.3) -->
    <xs:simpleType name="InventoryType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Nummer, die zur Fakturierung heranzuziehen ist
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Projektkenner  (W005.2, S005.2) -->
    <xs:simpleType name="ProjectIdType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Eindeutige Identifikation eines Projektes.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Kopplungskenner  (W005.3, S005.3) -->
    <xs:simpleType name="CorrelationIdType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Eindeutige Identifikation zur Kopplung mehrerer Auftraege innerhalb eines Projektes.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Vertragsnummer  (W006.2) -->
    <xs:simpleType name="ContractNoType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Anlagetyp  (W007.2) -->
    <xs:simpleType name="AttachmentType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Kennzeichnet die Art der Anlage
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>            
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Dateiname  (W007.3)  -->
    <xs:simpleType name="FilenameType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               File name with file type
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Datentyp   (W007.4) -->
    <xs:simpleType name="FileTypeType">
    	<xs:annotation>
            <xs:documentation xml:lang="en">
               
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="255"/>
            <xs:enumeration value="image/tiff"/>
            <xs:enumeration value="image/jpeg"/>
            <xs:enumeration value="application/msword"/>
            <xs:enumeration value="application/excel"/>
            <xs:enumeration value="application/pdf"/>
        </xs:restriction>
    </xs:simpleType>
     
    <!-- Beschreibung   (W007.5) -->
    <xs:simpleType name="DescriptionType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
            	Freitext zur Beschreibung des Inhaltes der Anlage
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="160"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Dateiinhalt   (W007.6) -->
    <xs:simpleType name="FileContentType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
            	Pfad zur Datei. Zulässige Dateiformate : .tif, .doc, .jpg, .xls, .pdf. Zulässige Dateigröße: max. 1 MB pro Anlage, max. 4 MB gesamt
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xsd:base64Binary">
        </xs:restriction>
    </xs:simpleType>

    <!-- Anrede (W008.2/W014.3) -->
    <xs:simpleType name="SalutationType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               1=Herr 2=Frau 9=keine Anrede 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="9"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Vorname (W008.3/W009.2/W010.4/W012.4/W013.4/W014.5/W048.4) -->
    <xs:simpleType name="GivenNameType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Vorname des Ansprechpartners/Vormieters 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Nachname (W008.4/W009.3/W010.3/W012.3/W013.3/W014.4/W048.3) -->
    <xs:simpleType name="SurNameType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Nachname des Ansprechpartners/Vormieters 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Emailadresse (W008.6/W014.7) -->
    <xs:simpleType name="EmailAddressType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Bsp.: Herbert.Mustermann@customer.de. Nur wenn Rolle vorhanden
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="320"/>
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value=".+@(.+\.)+\w+"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Rolle (W008.7) -->
    <xs:simpleType name="RoleType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Ansprechpartner des Kunden fuer vertriebliche, technische Nachfragen. 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
            <xs:enumeration value="Technik"/>
            <xs:enumeration value="Auftragsmanagement"/> 
            <xs:enumeration value="Endkunde"/>           
        </xs:restriction>
    </xs:simpleType>
    
    <!-- ONKZ (W009.4/W011.9/W026.2) -->
    <xs:simpleType name="AreaCodeType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Ortsnetzkennzahl des Anschlusses
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="5"/>
            <xs:pattern value="\d{2,5}"/>
        </xs:restriction>
    </xs:simpleType>

    
    <!-- UFANummer (W009.6) -->
    <xs:simpleType name="UFANumberType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
              UFA-Nummer
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="15"/> 
            <xsd:pattern value="\d{2,5}C\d{6,7}"/> 
            <xsd:pattern value="[0-9a-zA-Z]{1,15}"/>          
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Anrede StandortA (W010.2/W012.2/W013.2/W048.2) -->
    <xs:simpleType name="LocationAppellationType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
              1=Herr 2=Frau 4=Firma 9=keine Anrede 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:length value="1"/>
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="9"/>     
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Strassenname (W010.5/W011.2/W012.5/W013.5/W019.2/W031.2) -->
    <xs:simpleType name="StreetType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="40"/>       
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Hausnummer (W010.6/W011.3//W012.6/W013.6/W031.3) -->
    <xs:simpleType name="HouseNumberType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="4"/>       
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Hausnummernzusatz (W010.7/W011.4/W012.7/W013.7/W031.4) -->
    <xs:simpleType name="HouseNumberAddType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="6"/>       
        </xs:restriction>
    </xs:simpleType>

    <!-- GebaudeteilName (W010.8/W012.8/W013.8/W031.5) -->
    <xs:simpleType name="BuildingBlockNameType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Gebäudeteil am Standort
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:minLength value="1"/>
            <xs:maxLength value="25"/> 
        </xs:restriction>
    </xs:simpleType>

    <!-- GebaudeteilZusatz (W010.9/W012.9/W013.9/W031.6) -->
    <xs:simpleType name="BuildingBlockAppendixType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Gebäudeteilzusatz am Standort.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:minLength value="1"/>
            <xs:maxLength value="25"/> 
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Land (W010.10/W011.5/W012.10/W013.10/W031.7) -->
    <xs:simpleType name="CountryType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Vorbelegt mit Ländercode: DE  (Deutschland)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:length value="2"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Postleitzahl (W010.11/W011.6/W012.11/W013.11/W031.8) -->
    <xs:simpleType name="PostalCodeType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Vorbelegt mit Ländercode: DE  (Deutschland)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Ortsname (W010.12/W011.7/W012.12/W013.12/W031.9) -->
    <xs:simpleType name="CityType">
        <xs:annotation>
            <xs:documentation xml:lang="en">                
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:minLength value="1"/>
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Ortsteil (W010.13/W011.8/W012.13/W013.13/W031.10) -->
    <xs:simpleType name="CityAddType">
        <xs:annotation>
            <xs:documentation xml:lang="en">                
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- LageTAEDose (W010.14) -->
    <xs:simpleType name="taeLocationType">
        <xs:annotation>
            <xs:documentation xml:lang="en">                
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="160"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Anschlussbereich (W010.14) -->
    <xs:simpleType name="ConnectionAreaType">
        <xs:annotation>
            <xs:documentation xml:lang="en">   
                Anschlussbereich (ASB), in dem sich der Kollokationsraum befindet             
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="4"/>
            <xs:pattern value="\d{1,4}"/> 
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Montagekenner (W014.2) -->
    <xs:simpleType name="InstallationIdType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                f = frueh (7.00-9.00 h),v =vormittags (9.00-12.00 h),n = nachmittags (13.00-18.00 h),s = spät (18.00-20.00 h),m = ganztägig  (9.00-18.00 h), k= Keine Montage. Vorbelegt mit k            
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- Montageleistung (W014.8) -->
    <xs:simpleType name="InstallationServicesType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                Nähere Angaben zur Durchfuehrung der Montageleistung            
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">            
            <xs:maxLength value="160"/>
        </xs:restriction>
    </xs:simpleType>
 
    <!-- Datum (W015.2/W052.3/W030.4/W028.3/W043.2) -->
    <xs:simpleType name="DateType">
        <xs:annotation>
            <xs:documentation xml:lang="en">                    
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date">
            <xs:pattern value="\d{4}-.*"/>            
        </xs:restriction>
    </xs:simpleType>

    <!-- Zeitfenster (W015.3) -->
    <xs:simpleType name="TimeSlotType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
               Eingrenzung des KWT  oder kKWT auf definierte Std.-Intervalle. Die Bedeutung der IDs muessen
               in einer gesonderten Vereinbarung festgelegt werden. Die in den documentations angegebenen
               Werte sind spezifisch fuer die WITA-Schnittstelle der DT      
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        <xs:length value="1"/>
        <xsd:pattern value="[0-9a-z]"/>
        </xs:restriction>        
    </xs:simpleType>
    
    <!-- Zeitfenster (W033.2) -->
    <xs:simpleType name="TimeSlot2Type">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                     
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        	<xs:length value="2"/>
        	<xsd:pattern value="\d{1,2}"/>
    	</xs:restriction>
    </xs:simpleType>
    
    <!--  (W061) -->
    <xs:simpleType name="negotiationType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                     
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        	<xsd:minLength value="0"/>
            <xs:maxLength value="21"/> 
        	<xsd:pattern value="[0-9A-Z]{3}\.[0-9A-Z]{1,6}\.V[0-9A-Z]{9}"/>
    	</xs:restriction>
    </xs:simpleType>
    
       <!--  (W062) -->
    <xs:simpleType name="UserpinType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                 Encrypted PIN value
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        	<xsd:minLength value="0"/>
            <xs:maxLength value="1024"/>
    	</xs:restriction>
    </xs:simpleType>
    
     <!--  (W063) -->
    <xs:simpleType name="IptvprovideridType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                     
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        	<xsd:minLength value="1"/>
            <xs:maxLength value="15"/>
    	</xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="IptvbasicuserType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                     
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        	<xsd:minLength value="1"/>
            <xs:maxLength value="40"/>
    	</xs:restriction>
    </xs:simpleType>
    
     <!--  (W064) -->
    <xs:simpleType name="IptvuserchangeType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                     
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
        	<xsd:minLength value="0"/>
            <xs:maxLength value="1"/> 
        	<xsd:pattern value=".*"/>
    	</xs:restriction>
    </xs:simpleType>
    
      <!--  (W065) -->
    <xs:simpleType name="IptvsettingsType">
        <xs:annotation>
            <xs:documentation xml:lang="en">    
                     
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
